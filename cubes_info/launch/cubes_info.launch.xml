<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="use_sim_time" default="true" />
  <arg name="initial_delay" default="0.0" />

  <include file="$(find-pkg-share cubes_info)/launch/camera_36h11.launch.yml" />

  <node pkg="cubes_info" exec="find_cubes_info" name="find_cubes_info_node" output="screen"
    launch-prefix="bash -c 'sleep $(var initial_delay); $0 $@' ">
    <param name="use_sim_time" value="$(var use_sim_time)" />
  </node>
   
</launch>